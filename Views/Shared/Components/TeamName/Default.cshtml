@model IEnumerable<Teams>

<a class="btn btn-block btn-outline-secondary"
   asp-action="Index" asp-controller="Home" asp-route-teamid="">Home</a>
</br>

@foreach(var x in Model)
{
  <a 
     class="btn btn-block @(x.TeamName.ToString()==ViewBag.SelectedTeamName? "btn-primary" : "btn-outline-secondary")"
     asp-action="Index" asp-controller="Home" asp-route-teamid="@x.TeamId"
     asp-route-teamname="@x.TeamName" asp-route-pagenum="1"
     >@x.TeamName</a><br />
}